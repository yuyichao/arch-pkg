pkgname=julia-reexport-git
pkgver=0.0.3.0.7.g8ce0fd4
pkgrel=1
pkgdesc="Reexport.jl"
url="https://github.com/simonster/Reexport.jl"
arch=('any')
license=('MIT')
makedepends=('git' 'julia-pkg-scripts')
# TODO version depends
depends=('julia')
provides=('julia-reexport')
source=(git://github.com/simonster/Reexport.jl)
md5sums=('SKIP')

pkgver() {
  cd Reexport.jl

  git describe --tags | sed -e 's/^[^0-9]*//' -e 's/-/.0./' -e 's/-/./g'
}

package() {
  cd Reexport.jl

  . julia-install-pkg.sh Reexport "${pkgdir}" "${pkgname}"
}
