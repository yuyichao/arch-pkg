pkgname=julia-compat-git
pkgver=0.8.4.0.10.gbe6c6d0
pkgrel=1
pkgdesc="Compat.jl"
url="https://github.com/JuliaLang/Compat.jl"
arch=('any')
license=('MIT')
# TODO version depends
makedepends=('git' 'julia-pkg-scripts')
depends=('julia')
provides=('julia-compat')
source=(git://github.com/JuliaLang/Compat.jl)
md5sums=('SKIP')

pkgver() {
  cd Compat.jl

  git describe | sed -e 's/^[^0-9]*//' -e 's/-/.0./' -e 's/-/./g'
}

package() {
  cd Compat.jl

  . julia-install-pkg.sh Compat "${pkgdir}" "${pkgname}"
}
