pkgname=julia-iterators-git
pkgver=0.1.9.0.5.ge302a0d
pkgrel=1
pkgdesc="Iterators.jl"
url="https://github.com/JuliaLang/Iterators.jl"
arch=('any')
license=('MIT')
# TODO version depends
makedepends=('git' 'julia-pkg-scripts')
depends=('julia' 'julia-compat')
provides=('julia-iterators')
source=(git://github.com/JuliaLang/Iterators.jl)
md5sums=('SKIP')

pkgver() {
  cd Iterators.jl

  git describe | sed -e 's/^[^0-9]*//' -e 's/-/.0./' -e 's/-/./g'
}

package() {
  cd Iterators.jl

  . julia-install-pkg.sh Iterators "${pkgdir}" "${pkgname}"
}
