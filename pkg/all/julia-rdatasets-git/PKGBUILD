pkgname=julia-rdatasets-git
pkgver=0.0.0.55.1868811
pkgrel=2
pkgdesc="RDatasets.jl"
url="https://github.com/johnmyleswhite/RDatasets.jl"
arch=('any')
license=('MIT')
makedepends=('git' 'julia-pkg-scripts')
# TODO version depends
depends=('julia' 'julia-dataframes' 'julia-reexport' 'julia-compat')
provides=('julia-rdatasets')
source=(git://github.com/johnmyleswhite/RDatasets.jl)
md5sums=('SKIP')

pkgver() {
  cd RDatasets.jl

  echo "0.0.0.$(git rev-list --count master).$(git rev-parse --short master)"
}

package() {
  cd RDatasets.jl

  . julia-install-pkg.sh RDatasets "${pkgdir}" "${pkgname}"
}
